void setup(){
  
  size(1280,720);
 
}

void draw(){
  
  noStroke();
  fill(#ffffff);
  rect( 0, 0, 1280, 720);
  
  noStroke();
  fill(#ff2b2b);
  rect( 0, 0, 300, 720);
  
  noStroke();
  fill(#ff2b2b);
  rect( 980, 0, 300, 720);
  
  noStroke();
  fill(#ff2b2b);
  rect( 630, 420, 20, 120);
  
  triangle( 400, 450, 640, 120, 880, 450);
  
  triangle( 450, 450, 380, 200, 880, 450);
  
  triangle( 400, 450, 900, 200, 830, 450);
  
 
 
} 
  
 
